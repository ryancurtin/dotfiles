############ COLORS  ############### 
alias whitetext="osascript -e 'tell front window of app \"Terminal\" to set normal text color to {65535, 65535,65535, 65535}'"
alias whiteboldtext="osascript -e 'tell front window of app \"Terminal\" to set bold text color to {40000, 40000,40000, 65535}'"
alias greentext="osascript -e 'tell front window of app \"Terminal\" to set normal text color to {0, 65535, 30000, 65535}'"
alias colterm="whitetext && whiteboldtext && osascript -e 'tell front window of app \"Terminal\" to set background color to $1"
alias transterm="osascript -e 'tell application \"Terminal\" to set background color of window 1 to {0,0,0,-0.5}'"
alias defcurcol="osascript -e 'tell application \"Terminal\" to set cursor color of first window to {0,65535,15000}'"
alias defcolterm="greentext && osascript -e 'tell front window of app \"Terminal\" to set background color to $1"

# Default terminal color scheme (Homebrew)
alias defterm="defcurcol && defcolterm {0,0,0 }'"

